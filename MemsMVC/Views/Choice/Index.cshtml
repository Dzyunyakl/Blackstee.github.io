    <link href="~/css/animate.css" rel="stylesheet" media="all">
    <link href="~/css/style.css" rel="stylesheet" media="all">
</head>

<body>
    <div class="choice">
        <a class=" meme button button-block button-rounded button-highlight button-giant">Choose!</a>
    </div>
    <ul class="gallery">
        <li>
            <a asp-controller="Home" asp-action="Index"><img src="@ViewBag.First.Source" /><span class="name">GODNOTA</span></a>
        </li>
        <li>
            <a asp-controller="Home" asp-action="Index"><img src="@ViewBag.Second.Source" /><span class="name">GODNOTA</span></a>
